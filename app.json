{
  "addons": [
    "cleardb"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "env": {
    "CLEARDB_DATABASE_URL": {
      "required": true
    },
    "DB_CLIENT": {
      "required": true
    },
    "NODE_ENV": {
      "required": true
    },
    "NPM_CONFIG_PRODUCTION": {
      "required": true
    },
    "OMDB_API": {
      "required": true
    },
    "SENTRY_DSN": {
      "required": true
    },
    "TEST_DB_HOST": {
      "required": true
    },
    "TEST_DB_NAME": {
      "required": true
    },
    "TEST_DB_PASSWORD": {
      "required": true
    },
    "TEST_DB_USER": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "ng-rest",
  "scripts": {
  },
  "stack": "heroku-18"
}
